{
    "appenders": [
        {
            "type": "file",
            "category": "log",
            "filename": "logs/node_js.log",
            "backups": 30,
            "maxLogSize": "1024 * 1024 * 10",
            "pattern": "-yyyy-MM-dd"
        },
        {
            "type": "console",
            "category": "console"
        }
    ],
    "replaceConsole": true
}